<html>
<head>
<style type="text/css">
	/* Inspiration: https://github.com/zopyx/zopyx.smartprintng.plone/tree/cb1252601617324cf5513f65686dd4eb1e4b90a4/zopyx/smartprintng/plone/resources_default */
	
	* {
		box-sizing: border-box;
	}
	
	body {
		font-size: 11px;
		font-family: "ArialUnicode", Verdana, Arial, sans-serif;
		line-height: 1.5em;
	}

	div.header {
		display: block;
		text-align: center;
		position: running(header);
	}
	
	div.footer {
		display: block;
		text-align: center;
		position: running(footer);
	}
	
	div.breakBefore {
		page-break-before: always;
		clear: both;
	}
	
	div.breakAfter {
		page-break-after: always;
	}
	
	h1 {
		font-size: 200%;
		margin-top: 0;
		padding-bottom: 0px;
		line-height: 1.2em;
	}
	
	h2 {
		font-size: 133%;
		padding-bottom: 0px;
		line-height: 1.2em;
	}
	
	h3 {
		font-size: 115%;
		padding-top: 10px;
		padding-bottom: 0px;
		margin-bottom: 0px;
		line-height: 1.2em;
	}
	
	h4, h5, h6 {
		font-size: 100%;
		line-height: 1.2em;
	}

	p {
		line-height: 1.77em;
	}
	
	tr.total {
		font-weight: bold;
	} 
	
	pre {
		font-size: 0.775em;
		background-color: #eeeeee;
		color: black;
		white-space: pre-wrap;
		margin-left: 1em;
		margin-right: 1em;
		padding: 0.5em;
		font-family: "Courier New";
		border: thin solid black;
		border-radius: 8px;
		padding: 0.4em;
	}
	
	tt {
		font-family: Courier;
		background: #dddddd;
	}	

	table.bordered {
	    border-collapse: collapse;
	}
	
	table.bordered, table.bordered th, table.bordered td {
	    border: 1px solid black;
	}
	
	.row {
		margin-left: 0px;
		margin-right: 0px;
		padding: 0px;
		width: 100%;
		clear: both;
		display: inline-block;
	}
	
	.col-sm-1 { width: 8.3333%; }	
	.col-sm-2 { width: 16.6666%; }	
	.col-sm-3 { width: 25%; }
	.col-sm-4 { width: 33.3333%; }
	.col-sm-5 { width: 41.6666%; }	
	.col-sm-6 { width: 50%; }
	.col-sm-7 { width: 58.3333%; }	
	.col-sm-8 { width: 66.6666%; }
	.col-sm-9 { width: 75%; }	
	.col-sm-10 { width: 83.3333%; }	
	.col-sm-11 { width: 91.6666%; }	
	.col-sm-12 { width: 100%; }
	
	.col-sm-1,.col-sm-2,.col-sm-3,.col-sm-4,.col-sm-5,.col-sm-6,.col-sm-7,.col-sm-8,.col-sm-9,.col-sm-10,.col-sm-11,.col-sm-12
		{
		min-height: 1px;
		float: left;
		padding: 0px;
	}
	
	.lm-10 { margin-left: 10px; }
	.lm-20 { margin-left: 20px; }
	.lm-30 { margin-left: 30px; }
	.rm-10 { margin-right: 10px; }
	.rm-20 { margin-right: 20px; }
	.rm-30 { margin-right: 30px; }
	
	@page {
		size: A4 portrait;
		margin-top: 2.54cm;
		margin-bottom: 2.54cm;
		margin-left: 1.20cm;
		margin-right: 1.20cm;
	}
	
	@page { 
		margin: 80px 45px 45px;
		@top-center { 
			content:element(header)
		}
	}
	
	@page { 
		@bottom-right { 
			content:element(footer);
		}
	}
	
	#pagenumber:before {
		content: counter(page);
	}
	
	#pagecount:before {
		content: counter(pages);
	}
	
	.reportlisting {
		list-style: none;
	}
	
	img.reportImage{
		width: 100%;
	}
	
	.right {
		text-align: right;
	}
	
	/*
	* Tables with class "turntable" will be displayed in landscapeformat
	*/
	@page big_table {
		size: A4 landscape
	}
	
	table.turntable {
		page: big_table
	}
	
	.vilkaar {
		font-size: 9px;
	}
	
	.vilkaar-lille {
		font-size: 7px;
		line-height: 140%;
	}
	
	.med_smaat {
		font-size: 8px;
	}
	
	.med_smaat t3, .med_smaat td {
		height: 6px;
		line-height: 60%;
	}
	
	table td, table td * {
    	vertical-align: top;
    	text-align: left;
	}	
	
	.size10 {
		font-size: 10px;
	}
		
</style>
</head>

<body>
	<div class="panel-heading header">
		<h2>{$document_header:}</h2>
	</div>
	
	<div class="panel-footer footer">
		{$date:}   -   {$document_footer:}   -   Side <span id="pagenumber"></span> af <span id="pagecount"></span>
	</div>

{.if ($is_offer == TRUE) }
	<div class="row">
		<div class="col-sm-7">
			<h1>{$document_title:}</h1>
			<div class="lm-20 rm-20">
				{$customer_companyName}<br/>
				{$customer_name}<br/>
				{$customer_address}<br/>
				{$customer_zipCode} {$customer_city}<br/>
				<br/>
				{$intro_text}
				<br/>
				{$seller_companyName}<br/>
				{$seller_name}<br/>
				{$seller_phone}<br/>
				<br/>
				{$campaign_info}
			</div>
		</div>
		<div class="col-sm-5">
			<div class="media" data-src="{$main_image:}" style="width: 100%;"/>
		</div>
	</div>
	
	<div class="breakBefore"></div>
{/if}

{.if ($is_one_plus == TRUE) }
	<div class="row">
		<div class="col-sm-7">
			<h1>{$document_title:}</h1>
			<div class="lm-20 rm-20">
				<table style="width: 100%">
					<thead><tr><td width="10%"></td><td width="65%"></td><td width="25%"></td></tr></thead>

					{.loop in $oneTimeFeeLines as $line}
						{.if ($line.type == header) }
							<tr><td colspan="3"><h3>{$line.label}</h3></td></tr>
						{.elseIf ($line.type == discount) }
							<tr><td colspan="3" style="font-style: normal;">{$line.label}</td></tr>
						{.elseIf ($line.type == single_column) }
							<tr><td colspan="3">{$line.label}</td></tr>
						{.elseIf ($line.type == campaign_comment) }
							<tr><td colspan="3" style="font-style: normal;">{$line.label}</td></tr>
						{.elseIf ($line.type == space) }
							<tr><td colspan="3" style="min-height:1em"> </td></tr>
						{.elseIf ($line.type == total) }
							<tr class="total"><td>{$line.count}</td><td>{$line.label}</td><td class="right">{$line.amount}</td></tr>
						{.else}
							<tr><td>{$line.count}</td><td>{$line.label}</td><td class="right">{$line.amount}</td></tr>
						{/if}
					{.onEmpty}
					{/loop}
